{
  "name": "HonbabnonoApp",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "web": "webpack serve --mode development",
    "build:web": "NODE_ENV=production webpack --mode production",
    "server": "node server/index.js",
    "backend": "node backend/src/server.js",
    "dev": "concurrently \"npm run web\" \"npm run server\"",
    "dev:all": "concurrently \"npm run web\" \"npm run server\" \"npm run start\"",
    "ios:dev": "concurrently \"npm run web\" \"npm run server\" \"npm run start\" \"npm run ios\"",
    "metro": "react-native start --reset-cache",
    "test": "jest",
    "test:api": "NODE_ENV=test jest tests/",
    "test:simple": "NODE_ENV=test jest tests/simple.test.js",
    "test:unit": "NODE_ENV=test jest --selectProjects unit",
    "test:integration": "NODE_ENV=test jest --selectProjects integration",
    "test:flows": "NODE_ENV=test jest --selectProjects flows",
    "test:legacy": "NODE_ENV=test jest --selectProjects legacy",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:rice-index": "node server/test-rice-index.js",
    "ios:build": "cd ios && xcodebuild -workspace HonbabnonoApp.xcworkspace -scheme HonbabnonoApp -configuration Release -archivePath HonbabnonoApp.xcarchive archive",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "android:install": "cd android && ./gradlew installRelease",
    "build:all": "npm run android:build && npm run ios:build"
  },
  "dependencies": {
